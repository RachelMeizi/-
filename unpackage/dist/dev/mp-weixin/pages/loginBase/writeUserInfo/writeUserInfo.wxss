  min-height: 100rpx;
  border-bottom: 1rpx solid #F2F2F2;
}
.content-class {
  width: 90%;
  margin: 20rpx auto;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
