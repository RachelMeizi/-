          justify-content: space-between;
}
.content-class .item {
    width: 30%;
    height: 60rpx;
    font-size: 28rpx;
    line-height: 60rpx;
    border-radius: 30rpx;
    margin-bottom: 20rpx;
    text-align: center;
    box-sizing: border-box;
    border: 1rpx solid #3f82e7;
}
.content-class .item checkbox {
      display: none;
}
.content-class .on {
    border: none;
    background-color: #3f82e7;
    color: #fff;
}
.mask {
  position: fixed;
  z-index: 1000;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6);
}
