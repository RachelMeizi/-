
.header {
	margin: 90rpx 0 90rpx 50rpx;
	text-align: center;
	width: 650rpx;
	height: 300rpx;
	line-height: 450rpx;
}
.header image {
	width: 200rpx;
	height: 200rpx;
}
.content {
	margin-left: 50rpx;
	margin-bottom: 90rpx;
}
.content text {
	display: block;
	color: #9d9d9d;
	margin-top: 40rpx;
}
.bottom {
	border-radius: 80rpx;
	margin: 70rpx 50rpx;
	font-size: 35rpx;
	background: #F97C1A;
	color: white;
}
.comad {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	font-size: 26rpx;
	color: gray;
}
.comad>text {
	position: fixed;
	bottom: 100rpx;
}

